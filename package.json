    "Mahmoud Zakria (https://github.com/mahmoud-zakria-sociomantic)"
  ],
  "main": "dist",
  "sideEffects": false,
  "scripts": {
    "build": "yarn webpack:dev && yarn webpack:dist && yarn svg-sprite && yarn copy:sprite && yarn copy:styles",
    "bump:major": "yarn pre-bump && release-it major",
    "bump:minor": "yarn pre-bump && release-it minor",
    "bump": "yarn pre-bump && release-it",
    "copy:gh-pages": "shx cp -r .lochness/dist/* dist/displayComponents.js dist/displayComponentStyles.css docs/",
    "copy:sprite": "shx cp src/Icon/sprite.html dist/",
    "copy:styles": "shx cp dist/styles.css dist/styles.scss",
    "gh-pages": "yarn webpack:dev && yarn loch-ness:build && yarn copy:gh-pages",
    "loch-ness:build": "loch-build --libName=Nessie --lib=./ --src=./src --dist=./dist/index.dev.js --props=./src/defaults.json",
    "loch-ness:clean": "rimraf .lochness",
    "loch-ness": "loch --libName=Nessie --lib=./ --dist=./dist/index.dev.js",
    "pre-bump": "yarn build",
    "start": "yarn loch-ness:build && concurrently --kill-others \"yarn webpack:dev-watch\" \"yarn loch-ness\"",
    "svg-sprite": "./.prep-icon.sh && ./.generate-sprite.sh",
    "test:watch": "jest --watch --config=cfg/jest.config.js",
    "test": "jest --maxWorkers=4 --config=cfg/jest.config.js",
    "webpack:dev-watch": "webpack --env=dev --watch",
    "webpack:dev": "webpack --env=dev",
    "webpack:dist-watch": "webpack --env=dist --watch",
    "webpack:dist": "webpack --env=dist"
  },
  "keywords": [
    "css",
    "javascript",
    "nessie",
    "react",
    "scotland"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sociomantic-tsunami/nessie.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sociomantic-tsunami/nessie/issues"
  },
  "homepage": "https://github.com/sociomantic-tsunami/nessie#readme",
