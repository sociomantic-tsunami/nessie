{
  "name": "nessie-ui",
  "version": "4.1.0",
  "description": "Nessie React Components",
  "main": "dist",
  "scripts": {
    "build": "mkdir dist; yarn updateSprite && CSS_NANO=true yarn updateStyles",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "serve": "loch -e dev -s src -d dist -j src/defaults.json",
    "prepush": "yarn test:visuals",
    "dist": "webpack --env=dist",
    "dist-watch": "webpack --env=dist --watch",
    "dev": "webpack --env=dev --watch",
    "loch:build": "loch:build -e dev -s src -d dist -j src/defaults.json",
    "loch:clean": "rimraf .lochness",
    "start": "yarn loch:build && concurrently --kill-others \"yarn dev\" \"yarn serve\"",
    "test": "karma start",
    "test:v:ref": "visualTest -e dev -s src -d dist -j src/defaults.json -t reference",
    "test:Chrome": "karma start --singleRun=false --browsers Chrome",
    "test:v": "visualTest -e dev -s src -d dist -j src/defaults.json",
    "test:v:clean": "yarn loch:clean",
    "test:v:build": "yarn loch:build",
    "test:v:buildFresh": "concurrently \"yarn build\" \"yarn test:v:build\"",
    "test:visuals": "yarn test:v:clean && yarn build && yarn test:v:build && yarn test:v",
    "tdd": "karma start --reporters=mocha,notification --autoWatch=true --singleRun=false",
    "svgSprite": "./.prep-icon.sh  > /dev/null && ./.generate-sprite.sh  > /dev/null",
    "updateSprite": "yarn svgSprite && cp src/Icon/sprite.html dist/sprite.html && echo 'updated dist sprite'",
    "updateStyles": "yarn dist && cp dist/styles.css dist/styles.scss && echo 'updated nessie dist styles'",
    "bump": "yarn pre-bump && yarn release-it && yarn post-bump",
    "pre-bump": "git checkout Release && git pull && git merge master -X theirs --no-ff --no-edit && yarn updateStyles && yarn updateSprite && git add . && git commit -m 'compiled styles and sprite'",
    "post-bump": "git checkout master && git cherry-pick --strategy=recursive -X theirs Release && git push --no-verify"
  },
  "keywords": [
    "scotland",
    "nessie",
    "react",
    "css",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sociomantic-tsunami/nessie.git"
  },
